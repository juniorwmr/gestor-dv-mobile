{
  "expo": {
    "name": "Gestor DV",
    "slug": "gestordv",
    "version": "1.0.0",
    "privacy": "public",
    "orientation": "portrait",
    "platforms": ["ios", "android"],
    "icon": "./assets/bundle/logo.png",
    "splash": {
      "image": "./assets/bundle/Gestor.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "gestor.dv.ios"
    },
    "android": {
      "package": "gestor.dv"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}
